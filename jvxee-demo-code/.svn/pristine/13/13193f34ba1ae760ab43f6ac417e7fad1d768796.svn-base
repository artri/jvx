CREATE TABLE SALUTATION (ID BIGINT GENERATED BY DEFAULT AS IDENTITY (START WITH 1 INCREMENT BY 1) NOT NULL, SALUTATION VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE CUSTOMER (ID BIGINT GENERATED BY DEFAULT AS IDENTITY (START WITH 1 INCREMENT BY 1) NOT NULL, BIRTHDAY DATE, FILENAME VARCHAR(200), FIRSTNAME VARCHAR(255), IMAGE LONGVARBINARY, LASTNAME VARCHAR(255), PRIVATECUSTOMER BOOLEAN, EMAIL VARCHAR(255), TELEPHONEOFFICE VARCHAR(255), TELEPHONEPRIVATE VARCHAR(255), HEALTHINSURANCE_ID BIGINT, SALUTATION_ID BIGINT, PRIMARY KEY (ID))
CREATE TABLE HEALTHINSURANCE (ID BIGINT GENERATED BY DEFAULT AS IDENTITY (START WITH 1 INCREMENT BY 1) NOT NULL, CITY VARCHAR(255), HEALTHINSURANCE VARCHAR(255), STREET VARCHAR(255), ZIP INTEGER, PRIMARY KEY (ID))
CREATE TABLE ADDRESS (ID BIGINT GENERATED BY DEFAULT AS IDENTITY (START WITH 1 INCREMENT BY 1) NOT NULL, CITY VARCHAR(255), STREET VARCHAR(255), ZIP VARCHAR(4), CUSTOMER_ID BIGINT, PRIMARY KEY (ID))
CREATE TABLE EDUCATION (ID BIGINT GENERATED BY DEFAULT AS IDENTITY (START WITH 1 INCREMENT BY 1) NOT NULL, EDUCATION VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE CUSTOMER_EDUCATION (Customer_ID BIGINT NOT NULL, educations_ID BIGINT NOT NULL, PRIMARY KEY (Customer_ID, educations_ID))
