package com.sibvisions.rad.remote;

import javax.rad.remote.event.CallBackEvent;
import javax.rad.remote.event.ICallBackListener;

/**
 * The <code>NoCallBackListener</code> is an {@link ICallBackListener} but doesn't handle callback.
 * It ignores everything.
 * 
 * @author René Jahn
 */
public class NoCallBackListener implements ICallBackListener 
{
	//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
	// Interface implementation
	//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~

	@Override
	public void callBack(CallBackEvent pEvent) throws Throwable 
	{
		//We don't handle callbacks
	}

}	// NoCallBackListener
